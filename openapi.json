              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AccountInfo"
                }
              }
            }
          }
        }
      }
    },
    "/v1/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Public Api Health",
        "description": "Health check endpoint for the public API.\n\nUse this endpoint to verify that the API is operational and responding to requests.\n\nReturns:\n    dict: API health status and version information",
        "operationId": "public_api_health_v1_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    }
  },
